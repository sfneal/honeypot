{
    "name": "sfneal/honeypot",
    "description": "Prevent & track spam that's been submitted through public facing forms",
    "keywords": [
        "sfneal",
        "honeypot"
    ],
    "homepage": "https://github.com/sfneal/honeypot",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "spatie/laravel-honeypot": ">=2.1"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.3",
        "spatie/phpunit-snapshot-assertions": "^4.0",
        "spatie/test-time": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\Honeypot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\Honeypot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
